<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.codelabs.dokter_mobil_customer">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Doktermobilcustomer">
        <activity android:name=".page.member.ForgotPasswordVerificationActivity"></activity>
        <activity android:name=".page.member.ChangePasswordActivity"></activity>
        <activity android:name=".page.member.ForgotPasswordActivity" />
        <activity android:name=".page.walkthrough.WalkthroughActivity" />
        <activity android:name=".page.member.RegisterActivity" />
        <activity android:name=".page.member.LoginActivity" />
        <activity android:name=".page.splashscreen.SplashActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>